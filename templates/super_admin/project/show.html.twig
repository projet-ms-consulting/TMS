{% extends 'super_admin.html.twig' %}

{% block title %}Project{% endblock %}

{% block body %}
    <h1>Project</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Nom</th>
                <td>{{ project.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ project.description }}</td>
            </tr>
            <tr>
                <th>Participants</th>
                <td>{{ project.person }}</td>
            </tr>
            <tr>
                <th>Git</th>
                <td>{{ project.git }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ project.createdAt ? project.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ project.updatedAt ? project.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('super_admin_project_index') }}">Retour Ã  la liste</a>

    <a href="{{ path('super_admin_project_edit', {'id': project.id}) }}">Modifier</a>

    {{ include('super_admin/project/_delete_form.html.twig') }}
{% endblock %}
