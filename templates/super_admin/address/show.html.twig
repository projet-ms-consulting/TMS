{% extends 'super_admin.html.twig' %}

{% block title %}Address{% endblock %}

{% block body %}
    <h1>Address</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ address.id }}</td>
            </tr>
            <tr>
                <th>NbStreet</th>
                <td>{{ address.nbStreet }}</td>
            </tr>
            <tr>
                <th>Street</th>
                <td>{{ address.street }}</td>
            </tr>
            <tr>
                <th>ZipCode</th>
                <td>{{ address.zipCode }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ address.city }}</td>
            </tr>
            <tr>
                <th>Dernière modification</th>
                <td>{% if address.updatedAt != null %}
                        {{ address.updatedAt|date('Y-m-d H:i:s') }}
                    {% else %}
                        crée le : {{ address.createdAt|date('Y-m-d H:i:s')}}
                    {% endif %}
                </td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('super_admin_address_index') }}">back to list</a>

    <a href="{{ path('super_admin_address_edit', {'id': address.id}) }}">edit</a>

    {{ include('super_admin/address/_delete_form.html.twig') }}
{% endblock %}
