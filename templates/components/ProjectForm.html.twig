<div{{ attributes }}>
    {{ form_start(form) }}
    <table class="table">
        <tbody>
        <tr>
            <th>{{ form_label(form.name) }}</th>
            <td>{{ form_widget(form.name, {'attr': {'class': 'form-control'}}) }}</td>
        </tr>
        <tr>
            <th>{{ form_label(form.description) }}</th>
            <td>{{ form_widget(form.description, {'attr': {'class': 'form-control'}}) }}</td>
        </tr>
        <tr>
            <th>{{ form_label(form.company) }}</th>
            <td>{{ form_widget(form.company, {'attr': {'class': 'form-control'}}) }}</td>
        </tr>
        {% if form.participant is defined %}
            <tr>
                <th>{{ form_label(form.participant) }}</th>
                <td>{{ form_widget(form.participant, {'attr': {'class': 'form-control'}}) }}</td>
            </tr>
        {% endif %}
        <tr>
            <th>{{ form_label(form.linkGit) }}</th>
            <td>{{ form_widget(form.linkGit, {'attr': {'class': 'form-control'}}) }}</td>
        </tr>
        </tbody>
    </table>

    <div class="btnForm">
        <button id="btn-submit" class="btn btn-primary back-btn">Créer</button>
        <a href="{{ path('super_admin_project_index') }}" class="btn btn-primary back-btn">Retour</a>
    </div>
    {{ form_end(form) }}
</div>

